{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "cwd": "${workspaceFolder}/vscode-client",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": ["${workspaceFolder}/vscode-client/build/**/*.js", "!**/node_modules/**"],
      "preLaunchTask": "npm: build"
    }
  ]
}
